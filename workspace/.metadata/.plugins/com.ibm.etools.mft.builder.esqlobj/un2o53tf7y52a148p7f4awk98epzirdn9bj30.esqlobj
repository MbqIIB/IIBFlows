CREATE FILTER MODULE GetOrders_v1_Reply_Any_Errors__
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN

		DECLARE inRef REFERENCE TO Root.JSON.Data.orderResponse;		
		DECLARE errRef REFERENCE TO inRef.error[>];
		
		IF LASTMOVE(errRef) THEN
			RETURN TRUE;
		ELSE
			RETURN FALSE;
		END IF;
	
	END;

END MODULE;