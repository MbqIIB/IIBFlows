CREATE FILTER MODULE GetCustomer_v1_Reply_Any_Errors
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE inRef REFERENCE TO Root.JSON.Data.CustomerResponse;
		
		DECLARE errRef REFERENCE TO inRef.Item.error[>];
		
		IF LASTMOVE(errRef) THEN
			RETURN TRUE;
		ELSE
			RETURN FALSE;
		END IF;
	END;

END MODULE;